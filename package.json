{
  "name": "nodejs-jwt-auth-mongodb",
  "version": "1.0.0",
  "description": "Node.js + MongoDB: JWT Authentication & Authorization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "front": "cd front && npm start",
    "server-install": "npm install",
    "front-install": "cd front && npm install",
    "install-all": "concurrently \"npm run server-install\" \"npm run front-install\"",
    "dev": "concurrently \"npm run server\" \"npm run front\"",
    "heroku-postbuild": "cd front && npm install && npm run build"
  },
  "keywords": [
    "node.js",
    "express",
    "jwt",
    "auth",
    "mongodb"
  ],
  "author": "MVC",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.25.1",
    "concurrently": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
